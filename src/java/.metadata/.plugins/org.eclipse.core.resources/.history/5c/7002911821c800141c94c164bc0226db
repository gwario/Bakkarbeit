package callgrapher;

import static org.junit.Assert.*;

import org.junit.Test;

public class ColonDefinitionParserTest {

	@Test
	public void test_PATTERN_COLON_DEFINITION_START_1() {
		
		final String LINE = " : sdsd";
		
		assertTrue(LINE.matches(ColonDefinitionParser.PATTERN_COLON_DEFINITION_START));
		
	}

}
